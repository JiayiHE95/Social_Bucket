<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox minHeight="600.0" minWidth="125.0" prefHeight="600.0" prefWidth="125.0" spacing="10" style="-fx-background-color: transparent;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.polytech.SocialBucket.UI.SidebarController">
   <children>
      <Button onAction="#changeToWallet" style="-fx-background-color: transparent;" text="Wallet" underline="true">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button onAction="#changeToInfos" style="-fx-background-color: transparent;" text="Infos" underline="true">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button onAction="#changeToAdvertising" style="-fx-background-color: transparent;" text="Advertising" underline="true">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button onAction="#changeToChat" style="-fx-background-color: transparent;" text="Chat" underline="true">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font name="System Bold" size="16.0" />
         </font></Button>
      <Button onAction="#changeToSearchUsers" style="-fx-background-color: transparent;" text="Search Users" underline="true">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font name="System Bold" size="16.0" />
         </font></Button>
   </children>
   <padding>
      <Insets left="5.0" top="30.0" />
   </padding>
</VBox>
